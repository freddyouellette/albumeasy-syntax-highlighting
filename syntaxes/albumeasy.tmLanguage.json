{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "AlbumEasy",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"match": "#.*$",
					"name": "comment.line.hash.albumeasy"
				},
				{
					"name": "keyword.command.albumeasy",
					"match": "\\b(PAGE_TEXT|ALBUM_PAGES_SIZE|ALBUM_PAGES_MARGINS|ALBUM_PAGES_MARGINSE|ALBUM_PAGES_SPACING|ALBUM_PAGES_TITLE|ALBUM_PAGES_BORDER|ALBUM_PAGES_CROP_MARKS|ALBUM_PAGES_DECORATIVE_BORDER|ALBUM_PAGES_HEADER|ALBUM_PAGES_HEADER_NUMBER|ALBUM_PAGES_HEADER_DATE|ALBUM_PAGES_FOOTER|ALBUM_PAGES_FOOTER_NUMBER|ALBUM_PAGES_FOOTER_DATE|ALBUM_PAGES_NUMBER|ALBUM_PAGES_DATE|ALBUM_PAGES_HEADER_PAD|ALBUM_PAGES_FOOTER_PAD|ALBUM_PAGES_MARGIN_TXT_PAD|ALBUM_PAGES_MARGIN_TXT|ALBUM_AUTHOR|ALBUM_TITLE|PAGE_START|PAGE_START_VAR|PAGE_START_INFO|PAGE_COLUMN_START|PAGE_COLUMN_START|PAGE_COLUMN_NEXT|PAGE_COLUMN_STOP|PAGE_TEXT|PAGE_TEXT_CENTRE|PAGE_TEXT_RIGHT|PAGE_TEXT_PARAGRAPH_START|PAGE_TEXT_PARAGRAPH_END|PAGE_ADD_BOX|PAGE_QUADRILLE|PAGE_RULE_H|PAGE_VSPACE|PAGE_SET_VERTICAL_POS|PAGE_START_GROUP_BEGIN|PAGE_START_GROUP_END|PAGE_BACKGROUND_IMG|TEXT_CHAR_SPACING|TEXT_LINE_LEADING|ROW_START_ES|ROW_START_JS|ROW_START_FS|ROW_ALIGN_TOP|ROW_ALIGN_MIDDLE|ROW_ALIGN_BOTTOM|STAMP_ADD|STAMP_ADD_DIAMOND|STAMP_ADD_OVAL|STAMP_ADD_PENTAGON|STAMP_ADD_HEXAGON|STAMP_ADD_OCTAGON|STAMP_ADD_TRIANGLE|STAMP_ADD_TRIANGLE_INV|STAMP_ADD_RT_TRIANGLE|STAMP_ADD_IMG|STAMP_ADD_IMG|ALBUM_STAMP_IMG_NEW_SETTING|ALBUM_STAMP_IMG_NEW_SETTING_PCNT|ALBUM_STAMP_IMG_ASPECT_RATIO|ALBUM_STAMP_IMG_GREYSCALE_ON|ALBUM_STAMP_IMG_GRAYSCALE_ON|ALBUM_STAMP_IMG_GREYSCALE_OFF|ALBUM_STAMP_IMG_GRAYSCALE_OFF|ALBUM_STAMP_IMG_HIDE|ALBUM_STAMP_IMG_SHOW|STAMP_ADD_BLANK|STAMP_INNER_BORDER|STAMP_BORDER_STYLE|STAMP_INNER_BORDER_STYLE|STAMP_BOXES_SIZE_ADJUST|STAMP_HEADING|STAMP_HEADING_VA|STAMP_HEADING_PADDING|STAMP_FOOTER_TXT_ALIGN|STAMP_FOOTER_TXT_PAD|ALBUM_STAMP_SETTINGS_SAVE|ALBUM_STAMP_SETTINGS_RESTORE|COLOUR_ALBUM_BORDER|COLOUR_ALBUM_DECORATIVE_BORDER|COLOUR_ALBUM_FOOTER|COLOUR_ALBUM_HEADER|COLOUR_ALBUM_TITLE|COLOUR_ALBUM_MARGIN_TXT|COLOUR_PAGE_QUADRILLE|COLOUR_PAGE_RULE_H|COLOUR_PAGE_TEXT|COLOUR_STAMP_BORDER|COLOUR_STAMP_INNER_BORDER|COLOUR_STAMP_BACKGROUND|COLOUR_STAMP_HEADING|COLOUR_STAMP_TEXT|ALBUM_DEFINE_FONT)\\b"
				},
				{
					"name": "keyword.control.albumeasy",
					"match": "\\$\\b(INCLUDE|DEFINE|UNDEFINE|IFDEF|ELSE|ELSEIF|ENDIF)\\b"
				},
				{
					"name": "constant.other.albumeasy",
					"match": "\\$\\b([A-Fa-f0-9]{3}|[A-Fa-f0-9]{6})\\b"
				},
				{
					"name": "constant.other.albumeasy",
					"match": "\\b(aliceblue|antiquewhite|aqua|aquamarine|azure|beige|bisque|black|blanchedalmond|blue|blueviolet|brown|burlywood|cadetblue|chartreuse|chocolate|coral|cornflowerblue|cornsilk|crimson|cyan|darkblue|darkcyan|darkgoldenrod|darkgray|darkgreen|darkgrey|darkkhaki|darkmagenta|darkolivegreen|darkorange|darkorchid|darkred|darksalmon|darkseagreen|darkslateblue|darkslategray|darkslategrey|darkturquoise|darkviolet|deeppink|deepskyblue|dimgray|dimgrey|dodgerblue|firebrick|floralwhite|forestgreen|fuchsia|gainsboro|ghostwhite|gold|goldenrod|gray|grey|green|greenyellow|honeydew|hotpink|indianred|indigo|ivory|khaki|lavender|lavenderblush|lawngreen|lemonchiffon|lightblue|lightcoral|lightcyan|lightgoldenrodyellow|lightgray|lightgreen|lightgrey|lightpink|lightsalmon|lightseagreen|lightskyblue|lightslategray|lightslategrey|lightsteelblue|lightyellow|lime|limegreen|linen|magenta|maroon|mediumaquamarine|mediumblue|mediumorchid|mediumpurple|mediumseagreen|mediumslateblue|mediumspringgreen|mediumturquoise|mediumvioletred|midnightblue|mintcream|mistyrose|moccasin|navajowhite|navy|oldlace|olive|olivedrab|orange|orangered|orchid|palegoldenrod|palegreen|paleturquoise|palevioletred|papayawhip|peachpuff|peru|pink|plum|powderblue|purple|red|rosybrown|royalblue|saddlebrown|salmon|sandybrown|seagreen|seashell|sienna|silver|skyblue|slateblue|slategray|slategrey|snow|springgreen|steelblue|tan|teal|thistle|tomato|turquoise|violet|wheat|white|whitesmoke|yellow|yellowgreen)\\b"
				},
				{
					"match": "\\b\\d+(\\.\\d*)?\\b",
					"name": "constant.numeric.float.albumeasy"
				},
				{
					"match": "\\b(CN|CB|CI|CS|TN|TB|TI|TS|HN|HB|HI|HS|SC)\\b",
					"name": "constant.language.albumeasy"
				},
				{
					"match": "\\b(SOLID|DOTTED|DOT_DASH|DASHED|BLANK|CORNER|SIDES)\\b",
					"name": "constant.language.albumeasy"
				},
				{
					"match": "\\bALBUM_DEFINE_FONT\\s*\\(\\s*(\\w+)\\s*\"[^\"]+\"\\)",
					"captures": {
						"1": {
							"name": "constant.language.define.albumeasy"
						}
					}
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.albumeasy",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.albumeasy",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.albumeasy"
}